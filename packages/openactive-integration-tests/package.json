{
  "name": "openactive-integration-tests",
  "version": "1.0.0",
  "description": "OpenActive booking system integration tests",
  "main": "app.js",
  "scripts": {
    "start": "echo \"Error: package contains only tests\" && exit 1",
    "test": "jest",
    "debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand -- test/features/",
    "lint": "eslint \"test/features/**/*.js\"",
    "lint-fix": "eslint \"test/features/**/*.js\" --fix"
  },
  "author": "OpenActive Community <hello@openactive.io>",
  "license": "MIT",
  "eslintConfig": {},
  "dependencies": {
    "@openactive/data-model-validator": "^2.0.25",
    "@openactive/data-models": "^2.0.118",
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "config": "^3.2.4",
    "fs-extra": "^7.0.1",
    "handlebars": "^4.7.3",
    "mkdirp": "^1.0.3",
    "moment": "^2.24.0",
    "p-memoize": "^3.1.0",
    "rmfr": "^2.0.0",
    "strip-ansi": "^6.0.0",
    "sync-request": "^6.0.0"
  },
  "devDependencies": {
    "chakram": "^1.5.0",
    "jest": "^24.9.0",
    "jest-spec-reporter": "^1.0.10",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.20.0"
  },
  "jest": {
    "verbose": false,
    "testMatch": [
      "**/*-test.[tj]s?(x)"
    ],
    "reporters": [
      "<rootDir>test/reporter.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>test/setup.js"
    ],
    "globalSetup": "<rootDir>test/global-setup.js",
    "maxWorkers": 4,
    "testTimeout": 15000,
    "transform": {},
    "testEnvironment": "<rootDir>test/testEnvironment.js"
  }
}
